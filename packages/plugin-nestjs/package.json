{
  "name": "ssr-plugin-nestjs",
  "version": "6.2.4",
  "description": "plugin-nestjs for ssr",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently \"tsc -p ./tsconfig.cjs.json --inlineSourceMap false\" \" tsc -p ./tsconfig.esm.json --inlineSourceMap false\"",
    "build:only": "concurrently \"tsc-transpile-only -p ./tsconfig.cjs.json --inlineSourceMap false\" \" tsc-transpile-only -p ./tsconfig.esm.json --inlineSourceMap false\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.cjs.json \" \"tsc -w -p ./tsconfig.esm.json \"",
    "watch:cjs": "tsc -w -p ./tsconfig.cjs.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/algesthesiah/ssr.git"
  },
  "keywords": [
    "cli",
    "serverless",
    "ssr",
    "react"
  ],
  "author": "algesthesiah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/algesthesiah/ssr/issues"
  },
  "homepage": "https://github.com/algesthesiah/ssr#readme",
  "dependencies": {
    "ora": "^4.0.4",
    "rimraf": "^3.0.2",
    "cssr-server-utils": "^6.2.4"
  },
  "peerDependencies": {
    "@nestjs/common": ">=8",
    "@nestjs/core": ">=8"
  }
}
